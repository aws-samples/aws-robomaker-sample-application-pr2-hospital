    {
      "id": "Hospital_PR2_Cfg03",
      "name": "Hospital_PR2 Simulation",
      "type": "colcon build",
      "cfg": {
        "workingDir": "/aws-robomaker-hospital-PR2/simulation_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "Hospital_PR2_Cfg04",
      "name": "Hospital_PR2 Simulation",
      "type": "colcon bundle",
      "cfg": {
        "workingDir": "/aws-robomaker-hospital-PR2/simulation_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "Hospital_PR2_SimulationJob1",
      "name": "Hospital_PR2",
      "type": "simulation",
      "cfg": {
        "simulationApp": {
          "name": "RoboMakerHospital_PR2Simulation",
          "sourceBundleFile": "/aws-robomaker-hospital-PR2/simulation_ws/bundle/output.tar",
          "architecture": "X86_64",
          "s3Bucket": "<bucket name for your simulation bundle>",
          "launchConfig": {
            "packageName": "pr2_hospital_simulation",
            "launchFile": "hospital.launch",
            "environmentVariables": {
              "ROBOT_INITIAL_POSE": "-x 0.0 -y 10.0",
              "ROBOT": "sim"
            }
          },
          "simulationSoftwareSuite": {
            "name": "Gazebo",
            "version": "9"
