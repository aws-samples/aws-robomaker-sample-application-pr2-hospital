          "renderingEngine": {
            "name": "OGRE",
            "version": "1.x"
          },
          "robotSoftwareSuite": {
            "name": "ROS",
            "version": "Melodic"
          }
        },
        "simulation": {
          "maxJobDurationInSeconds": 28800,
          "failureBehavior": "Fail",
          "iamRole": "<your RoboMaker simulation job role ARN>",
          "outputLocation": "",
          "tags": {}
        }
      }
